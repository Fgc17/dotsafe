{
	"name": "fatima",
	"description": "typesafe secrets for the javascript ecosystem",
	"publishConfig": {
		"access": "public"
	},
	"version": "0.0.7",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Fgc17/fatima.git"
	},
	"files": ["dist"],
	"keywords": ["env", "typesafe", "typescript", "environment", "variable"],
	"type": "module",
	"main": "./dist/core/core.cjs",
	"types": "./dist/core/core.d.cts",
	"exports": {
		".": "./dist/core/core.cjs",
		"./env": "./dist/env/env.cjs"
	},
	"typesVersions": {
		"*": {
			".": ["./dist/core/core.d.cts"],
			"env": ["./dist/env/env.d.cts"]
		}
	},
	"bin": {
		"fatima": "./dist/cli/cli.cjs"
	},
	"scripts": {
		"typecheck": "tsc",
		"build": "rm -rf dist && tsc && pnpm tsup",
		"dev": "pnpm run build --watch",
		"release": "pnpm publish --no-git-checks"
	},
	"devDependencies": {
		"@types/node": "^22.9.1",
		"tsup": "^8.3.5",
		"typescript": "5.6.3"
	},
	"dependencies": {
		"@babel/plugin-transform-class-properties": "7.25.9",
		"@inquirer/prompts": "^7.2.4",
		"commander": "13.0.0",
		"jiti": "2.4.2"
	}
}
